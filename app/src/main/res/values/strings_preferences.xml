        <item>NORMAL</item>
        <item>LOW</item>
        <item>VERY_LOW</item>
        <item>OFF</item>
    </string-array>
    <string name="impact_sensitivity_default">NORMAL</string>

    <!-- ERS enabled -->
    <string name="ers_enabled_default">true</string>

