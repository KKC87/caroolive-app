<resources>

    <!-- Fuel Types -->
    <string-array name="fuel_type_entries">
        <item>@string/fuel_gasoline</item>
        <item>@string/fuel_diesel</item>
        <item>@string/fuel_lpg</item>
    </string-array>
    <string-array name="fuel_type_values">
        <item>GASOLINE</item>
        <item>DIESEL</item>
        <item>LPG</item>
    </string-array>
    <string name="fuel_type_default">GASOLINE</string>

    <!-- Engine Displacement -->
    <string name="engine_displacement_default">1600</string>

    <!-- Odometer -->
    <string name="odometer_default">0</string>

    <!-- Distance Unit -->
    <string-array name="distance_unit_entries">
        <item>@string/distance_unit_km</item>
        <item>@string/distance_unit_mi</item>
    </string-array>
    <string-array name="distance_unit_values">
        <item>KM</item>
        <item>MI</item>
    </string-array>
    <string name="distance_unit_default">KM</string>

