        <item>@string/video_quality_super_fine</item>
        <item>@string/video_quality_fine</item>
        <item>@string/video_quality_normal</item>
    </string-array>
    <string-array name="video_quality_values">
        <item>SUPER_FINE</item>
        <item>FINE</item>
        <item>NORMAL</item>
    </string-array>
    <string name="video_quality_default">NORMAL</string>

    <!-- Normal video duration -->
    <string-array name="normal_video_duration_entries">
        <item>@string/video_duration_1_mimute</item>
        <item>@string/video_duration_3_mimutes</item>
        <item>@string/video_duration_5_mimutes</item>
        <item>@string/video_duration_10_mimutes</item>
    </string-array>
    <string-array name="normal_video_duration_values">
        <item>60000</item>
        <item>180000</item>
        <item>300000</item>
        <item>600000</item>
    </string-array>
    <string name="normal_video_duration_default">180000</string>

    <!-- Storage type -->
    <string-array name="storage_type_entries">
        <item>@string/storage_internal</item>
        <item>@string/storage_external</item>
    </string-array>
    <string-array name="storage_type_values">
        <item>INTERNAL</item>
        <item>EXTERNAL</item>
    </string-array>
    <string name="storage_type_default">INTERNAL</string>

    <!-- Max storage size -->
    <string-array name="max_storage_size_entries">
        <item>@string/max_storage_size_full</item>
    </string-array>
    <string-array name="max_storage_type_values">
        <item>1099511627776</item>
        <!-- 1 TB -->
    </string-array>
    <string name="max_storage_size_default">1099511627776</string>

    <string-array name="blackbox_type_entries">
        <item>@string/blackbox_mr</item>
        <item>@string/blackbox_mc</item>
    </string-array>
    <string-array name="blackbox_type_values">
        <item>MEDIA_RECORDER</item>
        <item>MEDIA_CODEC</item>
    </string-array>

    <string-array name="auto_start_mode_marshmallow_entries">
        <item>@string/auto_start_mode_high</item>
        <item>@string/auto_start_mode_normal</item>
        <item>@string/auto_start_mode_low</item>
