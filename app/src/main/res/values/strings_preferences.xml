    <string-array name="fuel_economy_unit_entries">
        <item>@string/fuel_economy_unit_kpl</item>
        <item>@string/fuel_economy_unit_lp100km</item>
        <item>@string/fuel_economy_unit_mpg_us</item>
        <item>@string/fuel_economy_unit_mpg_uk</item>
