    <string-array name="speed_unit_entries">
        <item>@string/speed_unit_kph</item>
        <item>@string/speed_unit_mph</item>
    </string-array>
    <string-array name="speed_unit_values">
        <item>KPH</item>
        <item>MPH</item>
    </string-array>
    <string name="speed_unit_default">KPH</string>

    <!-- Volume Unit -->
    <string-array name="volume_unit_entries">
